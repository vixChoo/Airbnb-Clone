



<% if current_user.admin? || current_user.host? %>  

    <%= render partial:'application/navadmin' %>
    <br>
    <div class="table-responsive">
        <table class="table table-hover">

        <thead>
        <tr>
            <th>TOTAL Listings: <%=@listings.count %></th>

        <tr>
        </thead>
        <thead>
        <tr>
       
            <th>Listing Id</th>
            <th>Name</th>
            <th>Price</th>
            <th>Country</th>
            <th>Action</th>
        </tr>
        </thead>
        <tbody id="myDIV">
  
        <% @listings.order(created_at: :desc).each do |listing| %>
        <tr>
            <td><%= listing.id %></td>
            <td><%= link_to listing.name ,listing %></td> 
            <td><%= listing.price %></td>
            <td><%= listing.country %></td>
            <td><%= link_to 'Edit', edit_listing_path(listing) %>
            <%= link_to 'Destroy',listing,method: :delete, data: { confirm: 'Are you sure?' } %></td>
          <% end %>
          </tr> 
        </tbody>
    </table>
    </div>
<% end %>
        
           



        
      
       
